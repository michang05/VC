<?xml version="1.0" encoding="utf-8"?>

<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
	android:orientation="vertical" android:id="@+id/linearLayout1"
	android:layout_width="fill_parent" android:layout_height="wrap_content">
	<TextView android:layout_height="wrap_content"
		android:id="@+id/tvTransStatementSubTypeDescription"
		android:layout_width="wrap_content" android:text="TextView"
		android:textColor="@android:color/white" android:layout_marginTop="10sp"
		android:layout_marginLeft="20sp" android:textAppearance="?android:attr/textAppearanceMedium" />

	<TextView android:layout_height="wrap_content"
		android:id="@+id/tvTransactionStatementDescription"
		android:layout_width="wrap_content" android:text="TextView"
		android:textColor="#FFCC00" android:layout_marginLeft="30sp"
		android:textAppearance="?android:attr/textAppearanceSmall" android:layout_marginBottom="5sp" />
</LinearLayout>


