<?xml version="1.0" encoding="utf-8"?>
<LinearLayout android:id="@+id/linearTransactionTopBets"
	xmlns:android="http://schemas.android.com/apk/res/android"
	android:layout_width="wrap_content" android:layout_height="wrap_content"
	android:orientation="vertical">
	<TableLayout android:layout_width="fill_parent"
		android:layout_height="fill_parent" android:stretchColumns="1">
		<TableRow>
			<TextView android:textColor="@android:color/white"
				android:padding="3dip" android:id="@+id/tvEventsName"
				android:textAppearance="?android:attr/textAppearanceMedium" android:layout_width="wrap_content"/>
		</TableRow>
		<TableRow>
			<TextView android:textSize="14sp" android:layout_height="wrap_content"
				android:id="@+id/tvEventDateTime" android:layout_width="wrap_content"
				android:textColor="#FFCC00" android:padding="3dip" />
		</TableRow>
	</TableLayout>
</LinearLayout>


